{
  "name": "TippmixApp Dev Environment",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "postCreateCommand": "flutter doctor && flutter pub get && npm install --prefix functions",
  "customizations": {
    "vscode": {
      "extensions": [
        "Dart-Code.flutter",
        "esbenp.prettier-vscode",
        "ms-python.python"
      ]
    }
  },
  "remoteUser": "vscode"
}
